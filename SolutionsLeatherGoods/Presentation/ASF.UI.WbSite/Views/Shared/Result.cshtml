
@{
    ViewBag.Title = "Result";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Result</h2>

<div class="container body-content">
    <div class="row">
        <div>
            <h4>Opeartion_Result</h4>
        </div>
    </div>
    <div class="row">
        <div>
            @if (ViewBag.Result == "Operation_OK")
            {
                <h4 class="text-success">@ViewBag.Result</h4>
            }
            else
            {
                <h4 class="text-danger">@ViewBag.Result</h4>
            }
        </div>
    </div>
    <br />
    <div class="row">
        <div>
            @Html.ActionLink("Back", "Index", "Home", new { area=""}, new { @class = "btn btn-default" })
        </div>
    </div>
</div>
