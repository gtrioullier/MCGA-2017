@using ASF.UI.WbSite.Services.CultureHelper

@model ASF.Entities.Country

@{
    Layout = "~/Views/Shared/_Layout.cshtml";
    ViewBag.Title = "Details";
}

<h2>@Html.getResourceString("Details")</h2>

<div>
    <h4>@Html.getResourceString("Country")</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            @Html.DisplayName(Html.getResourceString("Name"))
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Name)
        </dd>

        <dt>
            @Html.DisplayName(Html.getResourceString("CreatedOn"))
        </dt>

        <dd>
            @Html.DisplayFor(model => model.CreatedOn)
        </dd>

        <dt>
            @Html.DisplayName(Html.getResourceString("CreatedBy"))
        </dt>

        <dd>
            @Html.DisplayFor(model => model.CreatedBy)
        </dd>

        <dt>
            @Html.DisplayName(Html.getResourceString("ChangedOn"))
        </dt>

        <dd>
            @Html.DisplayFor(model => model.ChangedOn)
        </dd>

        <dt>
            @Html.DisplayName(Html.getResourceString("ChangedBy"))
        </dt>

        <dd>
            @Html.DisplayFor(model => model.ChangedBy)
        </dd>

    </dl>
</div>
<p>
    @Html.ActionLink(Html.getResourceString("Edit"), "Edit", new { id = Model.Id }, new { @class = "btn btn-default" })
    @Html.ActionLink(Html.getResourceString("Back"), "Index", null, new { @class = "btn btn-default" })
</p>
